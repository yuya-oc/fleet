{
  "name": "fleet",
  "version": "0.0.1",
  "description": "KanColle browser",
  "main": "index.js",
  "scripts": {
    "postinstall": "cd app && yarn",
    "build": "run-p build:*",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack.config.main.js --optimize-minimize",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack.config.renderer.js --optimize-minimize",
    "pack": "run-p pack:*",
    "pack:win": "build --win --ia32 --em.main=main_bundle.js",
    "pack:mac": "build --mac --em.main=main_bundle.js",
    "watch": "run-p watch:*",
    "watch:main": "node scripts/watch_restart_electron.js app app/main.js app/index.html",
    "watch:renderer": "webpack-dev-server --config webpack.config.renderer.js --inline --history-api-fallback --content-base app/",
    "start": "electron app",
    "test": "eslint ."
  },
  "author": "Yuya Ochiai",
  "license": "MIT",
  "dependencies": {
    "dateformat": "^2.0.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-is-dev": "^0.1.2",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chokidar": "^1.6.1",
    "cross-env": "^3.1.3",
    "electron": "1.4.12",
    "electron-builder": "^10.11.0",
    "electron-connect": "^0.6.1",
    "eslint": "^3.12.0",
    "eslint-config-xo": "^0.17.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.8.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^3.1.2",
    "tree-kill": "^1.1.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^1.1.1"
  },
  "build": {
    "win": {
      "target": [
        "zip"
      ]
    },
    "mac": {
      "target": [
        "zip"
      ]
    }
  }
}

{
  "name": "fleet",
  "version": "0.0.1",
  "description": "KanColle browser",
  "main": "index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack.config.main.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack.config.renderer.js",
    "pack": "run-p pack:*",
    "pack:win": "build --win --ia32 --em.main=main_bundle.js",
    "pack:mac": "build --mac --em.main=main_bundle.js",
    "pack:linux": "build --linux --em.main=main_bundle.js",
    "watch": "run-p watch:*",
    "watch:main": "node scripts/watch_restart_electron.js app",
    "watch:renderer": "webpack-dev-server --config webpack.config.renderer.js --inline --history-api-fallback --content-base app/",
    "start": "electron app",
    "test": "run-s test:* lint:*",
    "watch-test": "chokidar app/**/*.js !app/node_modules/**/* test/**/*.js -c \"npm test -g --silent\"",
    "test:app": "./node_modules/.bin/mocha --recursive -r babel-register",
    "lint:js": "eslint ."
  },
  "author": "Yuya Ochiai",
  "license": "MIT",
  "dependencies": {
    "dateformat": "^2.0.0",
    "electron-is-dev": "^0.1.2",
    "electron-window-state": "^4.0.2",
    "http-proxy": "^1.16.2",
    "mkdirp": "^0.5.1",
    "photon": "https://github.com/connors/photon.git",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "stream-buffers": "^3.0.1",
    "strip-bom-buf": "^1.0.0",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.22.0",
    "babili-webpack-plugin": "^0.0.10",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "electron": "1.6.2",
    "electron-builder": "^10.11.0",
    "electron-connect": "^0.6.1",
    "electron-devtools-installer": "^2.1.0",
    "eslint": "^3.12.0",
    "eslint-config-xo": "^0.17.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.8.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.2",
    "power-assert": "^1.4.2",
    "style-loader": "^0.13.1",
    "tree-kill": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^2.6.1"
  },
  "build": {
    "win": {
      "target": [
        "zip"
      ]
    },
    "mac": {
      "target": [
        "zip"
      ]
    }
  }
}
